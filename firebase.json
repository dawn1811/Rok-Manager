{
  "functions": {
    "source": "functions",
    "runtime": "python311"
  },
  "hosting": {
    "public": "web/.next",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
